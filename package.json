    "prod:build": "bash scripts/deployment/production-build.sh",
    "prod:check": "node scripts/deployment/check-production-readiness.js",
    "prod:start": "NODE_ENV=production node dist/index.js",
    "deploy:build": "rm -rf node_modules package-lock.json && npm install && npm run build",
    "railway:dev": "railway run npm run dev",
    "railway:build": "npm ci --production=false --prefer-offline --no-audit --no-fund && npm run build",
    "check": "tsc",
    "lint": "npx tsc --noEmit",
    "audit:fix": "npm audit fix",
    "db:push": "drizzle-kit push",
    "db:push:supabase_disabled": "node scripts/migrate.js",
