# SafraReport Production Environment Variables for Railway Deployment

# API Base URL - will be set by Railway
# This should point to your deployed Railway app
VITE_API_BASE_URL=

# App Configuration for Dominican Republic
VITE_APP_NAME=SafraReport
VITE_APP_DOMAIN=safrareport.railway.app
VITE_DEFAULT_LOCALE=es-DO
VITE_DEFAULT_CURRENCY=DOP

# Feature flags for production
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_ANALYTICS=true

# Social Media for Dominican market
VITE_TWITTER_HANDLE=SafraReport
VITE_INSTAGRAM_HANDLE=safrareport
VITE_FACEBOOK_PAGE=SafraReportRD

# SEO optimized for Dominican users
VITE_DEFAULT_META_DESCRIPTION="Noticias, clasificados y reseñas de República Dominicana - SafraReport"
VITE_DEFAULT_META_KEYWORDS="noticias dominicanas, clasificados RD, reseñas negocios, SafraReport"

# Dominican specific features
VITE_ENABLE_DOP_CURRENCY=true
VITE_ENABLE_PROVINCE_FILTER=true
VITE_DEFAULT_TIMEZONE=America/Santo_Domingo