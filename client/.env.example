# SafraReport Client Environment Variables

# API Configuration
# For production deployment, set this to your backend URL
# In development, leave empty to use Vite proxy
VITE_API_BASE_URL=

# App Configuration
VITE_APP_NAME=SafraReport
VITE_APP_DOMAIN=safrareport.com

# Feature Flags
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_ANALYTICS=false

# Social Media
VITE_FACEBOOK_APP_ID=
VITE_TWITTER_HANDLE=SafraReport
VITE_INSTAGRAM_HANDLE=safrareport

# SEO
VITE_DEFAULT_META_DESCRIPTION="Noticias de República Dominicana - SafraReport"
VITE_DEFAULT_META_KEYWORDS="noticias, dominican republic, clasificados, reseñas"

# Deployment
# For Vercel/Netlify, set this to your production backend URL
# Example: https://your-backend-domain.com
# VITE_API_BASE_URL=https://safrareport-api.replit.app

# Supabase Configuration
# Replace these with your actual Supabase project details
VITE_SUPABASE_URL="https://your-project-id.supabase.co"
VITE_SUPABASE_ANON_KEY="your_supabase_anon_key"